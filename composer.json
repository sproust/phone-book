{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2.6",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/http": "^3.3",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"tracy/tracy": "^2.10",
		"nettrine/orm": "^0.10.0",
		"nettrine/dbal": "^0.10.2",
		"nettrine/cache": "^0.5.0",
		"nettrine/annotations": "^0.9.0",
		"nettrine/migrations": "^0.10.0",
		"nettrine/fixtures": "^0.8.0",
		"contributte/console": "^0.10.1",
		"symfony/cache": "^7.3",
		"webonyx/graphql-php": "^15.21"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^2",
		"symfony/thanks": "^1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"DataFixtures\\": "fixtures"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
