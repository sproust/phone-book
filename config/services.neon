services:
	- App\Application\Routing\RouterFactory::createRouter

	graphQLSchema:
		factory: GraphQL\Type\Schema
		arguments:
			config:
				query: @App\Infrastructure\GraphQL\Contact\ContactQuery
				mutation: @App\Infrastructure\GraphQL\Contact\ContactMutation


search:
	-	in: %appDir%
		classes:
			- *Facade
			- *Factory
			- *Repository
			- *Service
			- *UseCase
			- *Type
			- *Query
			- *Mutation
			- *Resolver
